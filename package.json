{
  "name": "minimal-ui-example",
  "version": "0.0.1",
  "main": "src/index.ts",
  "license": "MIT",
  "dependencies": {
    "@polkadot/keyring": "^9.4.1",
    "@polkadot/ui-shared": "^2.4.1",
    "@polkadot/wasm-util": "^6.3.1",
    "@polkadot/wasm-crypto-init": "^6.3.1",
    "@popperjs/core": "^2.11.5",
    "@reef-defi/extension-dapp": "^1.0.5",
    "@reef-defi/evm-provider": "^1.0.7",
    "ethers": "^5.6.9",
    "crypto-browserify": "^3.12.0",
    "esbuild": "^0.14.42",
    "events": "^3.3.0",
    "stream": "^0.0.2"
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --outfile=public/js/index.js --platform=browser --tsconfig=tsconfig.json",
    "start": "esbuild src/index.ts --bundle --outfile=public/js/index.js --platform=browser --watch --tsconfig=tsconfig.json",
    "serve": "esbuild src/index.ts --bundle --servedir=public --outdir=public/js --platform=browser --tsconfig=tsconfig.json"
  },
  "browser": {
    "crypto": false
  }
